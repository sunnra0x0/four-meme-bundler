# Four.Meme Bundler Configuration

# Blockchain Configuration
BSC_RPC_URL=https://bsc-dataseed.binance.org/
BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545/
PRIVATE_RPC_URL=https://api.ankr.com/v1/bsc/your_api_key
CREATOR_PRIVATE_KEY=your_creator_private_key_here
CREATOR_WALLET_ADDRESS=your_creator_wallet_address_here

# Buy Wallets Configuration
BUY_WALLETS_MNEMONIC=your_mnemonic_phrase_here
MAX_WALLETS=100
WALLET_FUNDING_AMOUNT=0.01

# Four.Meme Contract Addresses
FOUR_MEME_FACTORY=0x0000000000000000000000000000000000000000
FOUR_MEME_ROUTER=0x0000000000000000000000000000000000000000
WBNB_ADDRESS=0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c

# Bundler Configuration
MAX_BUNDLE_SIZE=50
MAX_GAS_PRICE=20
MIN_GAS_PRICE=1
BUNDLE_TIMEOUT=300000
LAUNCH_GAS_LIMIT=500000
BUY_GAS_LIMIT=200000

# MEV Protection Configuration
MEV_PROTECTION_ENABLED=true
PRIVATE_MEMPOOL=true
MEV_PROTECTION_DELAY=100
ANTI_FRONTRUN_ENABLED=true
ANTI_SANDWICH_ENABLED=true
GAS_PRICE_BUFFER=1.1
PRIORITY_FEE_BUFFER=1.2

# Gas Optimization Configuration
GAS_OPTIMIZATION_ENABLED=true
DYNAMIC_GAS_PRICING=true
GAS_PRICE_HISTORY_SIZE=100
NETWORK_CONGESTION_THRESHOLD=0.6
VALIDATOR_TIPS_ENABLED=true
VALIDATOR_TIP_AMOUNT=0.001

# Batch Processing Configuration
BATCH_PROCESSING_ENABLED=true
MAX_BATCH_SIZE=10
BATCH_TIMEOUT=300000
PARALLEL_BATCHES=3
RETRY_ATTEMPTS=3
RETRY_DELAY=1000
GAS_PRICE_INCREMENT=1.1
PRIORITY_FEE_INCREMENT=1.1

# Four.Meme API Configuration
FOUR_MEME_API_URL=https://api.four.meme
FOUR_MEME_API_KEY=your_four_meme_api_key_here

# Risk Management
MAX_RISK_SCORE=0.8
MAX_DAILY_LOSS=0.5
MAX_CONCURRENT_BUNDLES=5
PROFIT_THRESHOLD=0.01
MAX_SLIPPAGE=0.05

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/four-meme-bundler
REDIS_URL=redis://localhost:6379

# Monitoring Configuration
MONITORING_INTERVAL=5000
BUNDLE_MONITORING_ENABLED=true
PERFORMANCE_TRACKING=true

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/four-meme-bundler.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=5

# API Configuration
API_PORT=3001
API_HOST=localhost
CORS_ORIGIN=http://localhost:3000

# WebSocket Configuration
WS_PORT=3002
WS_HOST=localhost

# Notification Configuration
ENABLE_TELEGRAM_NOTIFICATIONS=false
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

ENABLE_DISCORD_NOTIFICATIONS=false
DISCORD_WEBHOOK_URL=your_discord_webhook_url

# Security Configuration
ENABLE_RATE_LIMITING=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Performance Configuration
ENABLE_CACHING=true
CACHE_TTL=300
MAX_CACHE_SIZE=1000

# Monitoring and Metrics
ENABLE_METRICS=true
METRICS_PORT=9090
HEALTH_CHECK_INTERVAL=30000

# Development Configuration
NODE_ENV=development
DEBUG=false
ENABLE_HOT_RELOAD=false
